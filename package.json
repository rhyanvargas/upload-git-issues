{
  "name": "upload-git-issues",
  "version": "1.0.0",
  "description": "A zero-config CLI tool to bulk upload CSV data to GitHub Issues",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "upload-git-issues": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test-parser": "node test-parser.js",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "keywords": [
    "github",
    "issues",
    "csv",
    "cli",
    "bulk-upload",
    "automation"
  ],
  "author": "Rhyan Vargas",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "csv-parser": "^3.0.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "jest": "^29.7.0",
    "tsx": "^4.6.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ]
}
